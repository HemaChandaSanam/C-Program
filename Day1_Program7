Value Performance
#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int a;
    float b;
    scanf("%d %f",&a,&b);
    printf("%d\n%0.2f\n",a,b);
    return 0;
}




